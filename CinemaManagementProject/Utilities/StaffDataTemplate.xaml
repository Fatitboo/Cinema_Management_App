                    xmlns:OrderFoodViewModel="clr-namespace:CinemaManagementProject.ViewModel.StaffVM.OrderFoodManagementVM"
                    xmlns:OrderFoodView="clr-namespace:CinemaManagementProject.View.Staff.OrderFoodManagement"
                    xmlns:FilmBookingView="clr-namespace:CinemaManagementProject.View.Staff.FilmBooking"
                    xmlns:FilmBookingViewModel="clr-namespace:CinemaManagementProject.ViewModel.StaffVM.FilmBookingVM">
    <DataTemplate DataType="{x:Type OrderFoodViewModel:OrderFoodManagementVM}">
        <OrderFoodView:OrderFoodPage></OrderFoodView:OrderFoodPage>
    </DataTemplate>
    <DataTemplate DataType="{x:Type FilmBookingViewModel:FilmBookingVM}">
        <FilmBookingView:FilmBookingPage></FilmBookingView:FilmBookingPage>
    </DataTemplate>
</ResourceDictionary>